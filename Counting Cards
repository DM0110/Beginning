let count = 0;

function cc(card) {
if (card > 1 && card < 7) {
  count ++;
}
else if (card === 10 || card === 'J' || card === 'Q' || card === 'K' || card === 'A') {
  count --;
}
if (count > 0) {
  return count + " Bet";
}
else {
  return count + " Hold";
}
  return "Change Me";
}
cc(2); // 1 Bet
cc(7); // 0 Hold
cc(10); // -1 Hold
cc('J'); // -1 Hold
